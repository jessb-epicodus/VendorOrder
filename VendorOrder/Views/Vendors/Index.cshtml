@{
  Layout = "_Layout";
}
@using VendorOrder.Models;
<h1>Vendors</h3><hr>
@if (@Model.Count == 0) {
  <p><em>No vendors have been added</em></p>
} 
@foreach (Vendor vendor in Model) {
  <h5><a href='/vendors/@vendor.Id'>@vendor.Name</a></h5>
}<br>
<p><a href='/vendors/new'>Add Vendor</a></p>
<p><a href='/'>Return Home</a></p>